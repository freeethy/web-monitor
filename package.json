  "scripts": {
    "start": "cross-env ENV=dev JS_ENV=development webpack-dev-server --mode development --color --config ./webpack.config.js --hot --inline --host 0.0.0.0",
    "start:build": "cross-env ENV=dev NEED_START=true JS_ENV=local webpack-dev-server --env=prod --mode production --color --config ./webpack.config.js --hot --inline --host 0.0.0.0",
    "build:dev": "cross-env ENV=dev JS_ENV=development webpack --env=prod --mode production",
    "build:test": "cross-env ENV=test JS_ENV=test webpack --env=prod --mode production",
    "build:staging": "cross-env ENV=staging JS_ENV=staging webpack --env=prod --mode production",
    "build:prod": "cross-env ENV=prod JS_ENV=production webpack --env=prod --mode production",
    "build:report": "cross-env ENV=dev NEED_REPORT=true webpack --env=prod --mode production",
    "dll": "webpack -p --progress --config webpack.dll.js"
  },
  "devDependencies": {
